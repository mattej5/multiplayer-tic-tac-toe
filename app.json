{
  "expo": {
    "name": "Multiplayer Tic Tac Toe",
    "slug": "mattej5-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mattej5app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/predevlock.png"
    },
    "android": {
      "icon": "./assets/images/predevlock.png",
      "predevlockIcon": {
        "foregroundImage": "./assets/images/predevlock.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mattej5.multiplayertictactoe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/predevlock.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c7946bf8-7338-46f4-8824-101fffa01008"
      }
    },
    "owner": "mattej5"
  }
}
